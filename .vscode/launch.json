  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker: Django",
      "type": "python",
      "request": "attach",
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}",
          "remoteRoot": "/app"
        }
      ],
      "port": 9876,
      "host": "127.0.0.1"
    },
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": ["runserver"],
      "django": true,
      "justMyCode": false
    },
    {
      "name": "Python: Django test",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "test",
        "care.emr.tests.test_meta_artifacts_api.TestMetaArtifactViewSet.test_retrieve_meta_artifact_permission_associated_to_encounter",
        "--keepdb"
      ],
      "django": true,
      "justMyCode": false
    }
  ]
}
